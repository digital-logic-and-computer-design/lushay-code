{
  "name": "lushay-code-constraints",
  "displayName": "Lushay Code Single-board Constraints",
  "description": "Open Source FPGA Toolchain using OSS-Cad-Suite",
  "version": "0.0.11",
  "license": "MIT",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "fpga"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lushaylabs/lushay-code.git"
  },
  "icon": "lushay-icon.png",
  "activationEvents": [
    "workspaceContains:**/*.cst",
    "workspaceContains:**/*.pcf",
    "workspaceContains:**/*.lpf",
    "onLanguage:verilog"
  ],
  "publisher": "bsiever",
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "lushay-code.constraintsEditor",
        "displayName": "Constraints Editor",
        "selector": [
          {
            "filenamePattern": "*.cst"
          },
          {
            "filenamePattern": "*.pcf"
          },
          {
            "filenamePattern": "*.lpf"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "bundle": "vsce package"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.1",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "^1.2.0",
    "serialport": "^10.5.0"
  }
}
